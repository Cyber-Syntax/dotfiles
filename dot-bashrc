# Use zsh as default shell, fallback to bash if zsh not available
if [[ $(ps --no-header --pid=$PPID --format=cmd) != "zsh" ]]; then
  if command -v zsh >/dev/null 2>&1; then
    exec zsh
  else
    echo "zsh not found, using bash instead"
  fi
fi

# AutoTarCompress completion
source "$HOME/.local/share/autotarcompress/autocomplete/bash_autocomplete"
# >>> my-unicorn completion start >>>
MY_UNICORN_COMPLETION_DIR="$HOME/.local/share/my-unicorn/autocomplete"
if [ -f "$MY_UNICORN_COMPLETION_DIR/bash_autocomplete" ]; then
  source "$MY_UNICORN_COMPLETION_DIR/bash_autocomplete"
fi
# <<< my-unicorn completion end <<<
# >>> aps completion start >>>
APS_COMPLETION_DIR="$HOME/.local/share/aps/autocomplete"
if [ -f "$APS_COMPLETION_DIR/bash_autocomplete" ]; then
  source "$APS_COMPLETION_DIR/bash_autocomplete"
fi
# <<< aps completion end <<<
